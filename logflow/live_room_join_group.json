{
  "name": "join group流程",
  "desc": "进入房间join group",
  "msg": {
    "enter": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "enterRoom",
      "desc": "enter room"
    },
    "join_living": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "[RoomLogin] joinLiving,",
      "desc": "start join living"
    },
    "join_living_success": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "[RoomLogin] handleJoinMediaGroupCallRes:",
      "desc": "join living success"
    },
    "leave": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoom ",
      "desc": "leave room"
    },
    "error": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoomForError",
      "desc": "leave room with error"
    }
  },
  "state": [
    {
      "type": "start",
      "name": "START"
    },
    {
      "name": "PrepareForJoin"
    },
    {
      "name": "JoiningLive"
    },
    {
      "name": "InRoom"
    },
    {
      "type": "end",
      "name": "END"
    },
    {
      "type": "error",
      "name": "ERROR"
    }
  ],
  "link": [
    {
      "from": [
        "START"
      ],
      "to": "PrepareForJoin",
      "msg": "enter",
      "desc": "进入房间"
    },
    {
      "from": [
        "PrepareForJoin"
      ],
      "to": "JoiningLive",
      "msg": "join_living",
      "desc": "发起join group"
    },
    {
      "from": [
        "JoiningLive"
      ],
      "to": "InRoom",
      "msg": "join_living_success",
      "desc": "join group 成功"
    },
    {
      "from": [
        "InRoom",
        "PrepareForJoin",
        "JoiningLive"
      ],
      "to": "END",
      "msg": "leave",
      "desc": "关闭房间"
    },
    {
      "from": [
        "InRoom",
        "PrepareForJoin",
        "JoiningLive"
      ],
      "to": "ERROR",
      "msg": "error",
      "desc": "因错误退出房间"
    },
    {
      "from": [
        "InRoom"
      ],
      "to": "ERROR",
      "msg": "enter",
      "desc": "重复打开房间"
    }
  ]
}