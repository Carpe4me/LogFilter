{
  "name": "观众join group流程",
  "desc": "观众进入房间join group",
  "msg": {
    "enter": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "enterRoom",
      "desc": "enter room"
    },
    "init_owner_stat": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "init owner stat",
      "desc": "init owner stat"
    },
    "join_living": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "[RoomLogin] joinLiving,",
      "desc": "start join living"
    },
    "join_living_success": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "[RoomLogin] handleJoinMediaGroupCallRes:",
      "desc": "join living success"
    },
    "leave": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoom ",
      "desc": "leave room"
    },
    "owner_not_in_room": {
      "tag": "RoomProXLog",
      "match_type": "contains",
      "pattern": "isOwnerInRoom:false",
      "desc": "owner leave room already"
    },
    "video_end": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoomForError:0",
      "desc": "room video end"
    },
    "error": {
      "tag": "RoomProXLog",
      "match_type": "regex",
      "pattern": "leaveRoomForError:[^0]",
      "desc": "leave room with error"
    }
  },
  "state": [
    {
      "type": "start",
      "name": "START"
    },
    {
      "name": "PrepareForJoin"
    },
    {
      "name": "JoiningLive"
    },
    {
      "name": "InRoom"
    },
    {
      "type": "end",
      "name": "END"
    },
    {
      "type": "error",
      "name": [
        "ERROR",
        "IsOwner"
      ]
    }
  ],
  "link": [
    {
      "from": [
        "START"
      ],
      "to": "PrepareForJoin",
      "msg": "enter",
      "desc": "进入房间"
    },
    {
      "from": [
        "PrepareForJoin"
      ],
      "to": "JoiningLive",
      "msg": "join_living",
      "desc": "发起join group"
    },
    {
      "from": [
        "PrepareForJoin"
      ],
      "to": "IsOwner",
      "msg": "init_owner_stat",
      "addToResultIfError": false,
      "removeResultIfError": true,
      "desc": "当前是主播流程"
    },
    {
      "from": [
        "JoiningLive",
        "PrepareForJoin"
      ],
      "to": "InRoom",
      "msg": "join_living_success",
      "desc": "join group 成功"
    },
    {
      "from": [
        "InRoom",
        "PrepareForJoin",
        "JoiningLive"
      ],
      "to": "END",
      "msg": "leave",
      "desc": "关闭房间"
    },
    {
      "from": [
        "InRoom",
        "PrepareForJoin",
        "JoiningLive"
      ],
      "to": "END",
      "msg": [
        "video_end",
        "owner_not_in_room"
      ],
      "desc": "直播结束(ROOM_ERR_VIDEO_END 或者 isOwnerInRoom=false)"
    },
    {
      "from": [
        "InRoom",
        "PrepareForJoin",
        "JoiningLive"
      ],
      "to": "ERROR",
      "msg": "error",
      "desc": "因错误退出房间"
    }
  ]
}