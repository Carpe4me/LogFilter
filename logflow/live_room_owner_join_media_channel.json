{
  "index": 6,
  "name": "主播join media流程",
  "desc": "主播进入房间媒体相关过程",
  "msg": {
    "init_owner_stat": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "init owner stat",
      "desc": "init owner stat"
    },
    "start_media": {
      "tag": "MediaSdkManagerRoomProXLog",
      "match_type": "startsWith",
      "pattern": "[issue] mMedia.",
      "desc": "start media"
    },
    "start_video": {
      "tag": "MediaSdkManagerRoomProXLog",
      "match_type": "startsWith",
      "pattern": "[issue] mVideo.",
      "desc": "start video"
    },
    "set_live_type": {
      "tag": "MediaSdkManagerRoomProXLog",
      "match_type": "startsWith",
      "pattern": "setLiveType:",
      "desc": "设置直播类型"
    },
    "media_connected": {
      "tag": "MediaSdkManagerRoomProXLog",
      "match_type": "startsWith",
      "pattern": "OnMediaSDKStatusListener MEDIA_SERVER_CONNECTED",
      "desc": "media server connected"
    },
    "video_connected": {
      "tag": "MediaSdkManagerRoomProXLog",
      "match_type": "startsWith",
      "pattern": "OnVideoStatusListener OP_NETWORK_CONNECTED",
      "desc": "video server connected"
    },
    "leave": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoom ",
      "desc": "leave room"
    },
    "video_end": {
      "tag": "RoomProXLog",
      "match_type": "startsWith",
      "pattern": "leaveRoomForError:0",
      "desc": "room video end"
    },
    "error": {
      "tag": "RoomProXLog",
      "match_type": "regex",
      "pattern": "leaveRoomForError:[^0]",
      "desc": "leave room with error"
    }
  },
  "state": [
    {
      "type": "start",
      "name": "START"
    },
    {
      "name": [
        "Preparing",
        "MediaStarted",
        "VideoStarted",
        "LiveTypeSet",
        "MediaConnected",
        "VideoConnected"
      ]
    },
    {
      "type": "end",
      "name": "END"
    },
    {
      "type": "error",
      "name": "ERROR"
    }
  ],
  "link": [
    {
      "from": [
        "START"
      ],
      "to": "Preparing",
      "msg": "init_owner_stat",
      "desc": "主播初始化media"
    },
    {
      "from": [
        "Preparing"
      ],
      "to": "MediaStarted",
      "msg": "start_media",
      "desc": "启动media"
    },
    {
      "from": [
        "MediaStarted"
      ],
      "to": "VideoStarted",
      "msg": "start_video",
      "desc": "启动video"
    },
    {
      "from": [
        "VideoStarted"
      ],
      "to": "LiveTypeSet",
      "msg": "set_live_type",
      "desc": "已设置直播类型"
    },
    {
      "from": [
        "LiveTypeSet",
        "MediaConnected"
      ],
      "to": "VideoConnected",
      "msg": "video_connected",
      "desc": "vs连接成功"
    },
    {
      "from": [
        "LiveTypeSet",
        "VideoStarted",
        "VideoConnected"
      ],
      "to": "MediaConnected",
      "msg": "media_connected",
      "desc": "ms连接成功"
    },
    {
      "from": [
        "VideoConnected",
        "MediaConnected"
      ],
      "to": "END",
      "msg": "leave",
      "desc": "退出房间"
    },
    {
      "from": [
        "Preparing",
        "MediaStarted",
        "VideoStarted",
        "LiveTypeSet",
        "MediaStarted",
        "VideoStarted",
        "MediaConnected",
        "VideoConnected"
      ],
      "to": "END",
      "msg": "video_end",
      "desc": "直播结束(ROOM_ERR_VIDEO_END)"
    },
    {
      "from": [
        "Preparing",
        "MediaStarted",
        "VideoStarted",
        "LiveTypeSet",
        "MediaStarted",
        "VideoStarted"
      ],
      "to": "ERROR",
      "msg": "leave",
      "desc": "没出连接上ms或者vs就退出房间"
    },
    {
      "from": [
        "Preparing",
        "MediaStarted",
        "VideoStarted",
        "LiveTypeSet",
        "MediaStarted",
        "VideoStarted",
        "MediaConnected",
        "VideoConnected"
      ],
      "to": "ERROR",
      "msg": "error",
      "desc": "因错误退出房间"
    }
  ]
}